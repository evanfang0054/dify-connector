{
  "name": "dify-connector",
  "version": "1.3.0",
  "description": "一个全面的 Dify AI 平台 Node.js 连接器，具有知识库管理和流式处理能力",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsx build.ts",
    "build:dev": "tsx build.ts dev",
    "build:types": "tsx build.ts types",
    "build:clean": "tsx build.ts clean",
    "build:analyze": "tsx build.ts && node -e \"console.log('ESM Bundle Analysis:'); console.log(JSON.stringify(JSON.parse(require('fs').readFileSync('./dist/esm/metafile.json', 'utf8')), null, 2)); console.log('\\nCJS Bundle Analysis:'); console.log(JSON.stringify(JSON.parse(require('fs').readFileSync('./dist/cjs/metafile.json', 'utf8')), null, 2));\"",
    "clean": "tsx build.ts clean",
    "type-check": "tsc --noEmit",
    "prebuild": "pnpm run clean",
    "test-connector": "tsx tests/test-connector.ts",
    "test-streaming": "tsx tests/test-streaming.ts",
    "test-node-streaming": "tsx tests/test-node-streaming.ts",
    "test-all": "tsx tests/test-all-functions.ts",
    "test-file-upload": "tsx tests/test-file-upload.ts",
    "test-workflow": "tsx tests/test-workflow.ts",
    "test-workflow-node-stream": "tsx tests/test-workflow-node-stream.ts",
    "test-actual": "tsx tests/test-actual-functionality.ts",
    "test-knowledge-base": "tsx tests/test-knowledge-base.ts",
    "test-config-params": "tsx tests/test-config-params.ts",
    "example": "tsx tests/example.ts"
  },
  "keywords": [
    "dify",
    "ai",
    "connector",
    "api",
    "knowledge-base",
    "workflow",
    "streaming",
    "chatbot",
    "typescript",
    "nodejs"
  ],
  "author": "evanfang",
  "license": "MIT",
  "dependencies": {
    "@microsoft/fetch-event-source": "^2.0.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.0",
    "eventsource": "^4.0.0",
    "form-data": "^4.0.4",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "esbuild": "^0.25.8",
    "tslib": "^2.8.1",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20"
  }
}